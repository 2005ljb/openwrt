/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "qca9557.dtsi"

/ {
	aliases {
		led-boot = &user;
		led-failsafe = &user;
		led-running = &user;
		led-upgrade = &user;
	};

	chosen {
		bootargs = "console=ttyS0,115200n8";
	};

	leds: leds {
		compatible = "gpio-leds";

		user: user {
			label = "rb:green:user";
			gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
	};

	keys: keys {
		compatible = "gpio-keys-polled";
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio 20 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
			debounce-interval = <60>;
		};
	};
};

&spi {
	status = "okay";
	num-cs = <1>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "routerboot";
				reg = <0x0000 0xc000>;
				read-only;
			};

			partition@1 {
				label = "hard_config";
				reg = <0xc000 0xd000>;
			};

			partition@2 {
				label = "bios";
				reg = <0xd000 0xe000>;
			};

			partition@3 {
				label = "soft_config";
				reg = <0xd000 0xf000>;
			};
		};
	};

	flash@1 {
		compatible = "spinand,mt29f";
		reg = <1>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "booter";
				reg = <0x0000000 0x0040000>;
			};

			partition@1 {
				label = "kernel";
				reg = <0x0040000 0x0400000>;
			};

			partition@2 {
				label = "ubi";
				reg = <0x0400000 0x8000000>;
			};
		};
	};
};

&pcie0 {
	status = "okay";
};

&uart {
	status = "okay";
};
