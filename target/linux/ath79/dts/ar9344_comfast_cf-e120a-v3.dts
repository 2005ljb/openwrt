/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "ar9344.dtsi"

/ {
	compatible = "comfast,cf-e120a-v3", "qca,ar9344";
	model = "COMFAST CF-E120A v3";

	aliases {
		serial0 = &uart;
		led-boot = &wan;
		led-failsafe = &wan;
		led-upgrade = &wan;
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-0 = <&led_rssimediumhigh_pin>;

		wan: wan {
			label = "cf-e120a-v3:green:wan";
			gpios = <&gpio 3 GPIO_ACTIVE_LOW>;
		};

		lan {
			label = "cf-e120a-v3:green:lan";
			gpios = <&gpio 2 GPIO_ACTIVE_LOW>;
		};

		rssilow {
			label = "cf-e120a-v3:red:rssilow";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
		};

		rssimediumlow {
			label = "cf-e120a-v3:red:rssimediumlow";
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
		};

		rssimediumhigh {
			label = "cf-e120a-v3:green:rssimediumhigh";
			gpios = <&gpio 16 GPIO_ACTIVE_HIGH>;
		};

		rssihigh {
			label = "cf-e120a-v3:green:rssihigh";
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
		};
	};

	ath9k-leds {
		compatible = "gpio-leds";

		wlan {
			label = "cf-e120a-v3:green:wlan";
			gpios = <&gpio 0 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};
	};

	keys {
		compatible = "gpio-keys-polled";
		poll-interval = <20>;

		reset {
			label = "Reset button";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 22 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};
};

&pinmux {
		led_rssimediumhigh_pin: pinmux_rssimediumhigh_pin {
			pinctrl-single,bits = <0x10 0x0 0xff>;
		};
};

&ref {
	clock-frequency = <40000000>;
};

&uart {
	status = "okay";
};
&spi {
	status = "okay";
	num-cs = <1>;

	flash@0 {
		compatible = "eon,en25qh64", "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			uboot:	partition@0 {
				label = "u-boot";
				reg = <0x000000 0x010000>;
				read-only;
			};

			art: partition@10000 {
				label = "art";
				reg = <0x010000 0x010000>;
				read-only;
			};

			firmware: partition@20000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x020000 0x7d0000>;
			};

			nvram: partition@ff0000 {
				label = "nvram";
				reg = <0x7f0000 0x010000>;
				read-only;
			};
		};
	};
};

&uart {
	status = "okay";
};

&mdio0 {
	status = "okay";

	phy-mask = <0x10>;

	phy0: ethernet-phy@0 {
		reg = <0>;
	};
};

&eth0 {
	status = "okay";

	pll-data = <0xe000000 0x04000101 0x04001313>;

	mtd-mac-address = <&art 0x0>;

	phy-mode = "rgmii-rxid";
	phy-handle = <&phy0>;

	gmac-config {
		device = <&gmac>;
		rgmii-gmac0 = <1>;
		rxd-delay = <1>;
		rxdv-delay = <1>;
	};
};

&eth1 {
	status = "okay";
	mtd-mac-address = <&art 0x1002>;
};

&wmac {
	status = "okay";
	mtd-cal-data = <&art 0x1000>;
	mtd-mac-address = <&art 0x6>;
};
